{
  "auth_supported": [ "Api Key", "OAUTH2" ],
  "auth_specification": {
    "Api Key": {
      "inputSpecification": {
        "type": "object",
        "properties": {
          "credentials": {
            "type": "object",
            "properties": {
              "api_key": {
                "type": "string",
                "title": "Api Key",
                "description": "Enter the API Key"
              }
            }
          }
        }
      },
      "headers": {
        "Authorization": "token {api_key}"
      }
    },
    "OAUTH2": {
      "token_url": "https://github.com/login/oauth/access_token"
    }
  }
}