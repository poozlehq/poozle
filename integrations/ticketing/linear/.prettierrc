{
  "arrowParens": "always",
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "requirePragma": false,
  "proseWrap": "preserve",
  "singleQuote": true,
  "formatOnSave": true,
  "trailingComma": "all",
  "printWidth": 100,
  "overrides": [
    {
      "files": ".prettierrc",
      "options": {
        "parser": "json"
      }
    }
  ]
}
